

<%- include("../partials/layout-start", {title: title}) %>
<%- include("../partials/menu") %>
<h1><%= title %></h1>

<div class="row">
    <div class="col-sm-8">
        <p>
            ID: <%= user.id %><br />
            Username: <%= user.userName %><br />
            Name: <%= user.displayName %><br />
